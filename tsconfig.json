{
    "compilerOptions": {
      "outDir": "./dist/",
      "noImplicitAny": true,
      "module": "esnext",
      "target": "es5",
      "jsx": "react-jsx",
      "baseUrl": ".",
      "paths": {
        "*": ["./src/*"]
      },
      "allowJs": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
    },
    "include": ["src", "custom.d.ts", "./config/jest/setupTests.ts"],
    "ts-node": {
    "compilerOptions": {
      "module": "CommonJS"
    },
  }
  }

